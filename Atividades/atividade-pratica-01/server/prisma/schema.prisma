generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  Id          Int         @id @default(autoincrement())
  name        String  
  email       String      @unique
  password    String
  created_at  DeteTime    @default(now())
  updated_at  DeteTime    @default(now())
}

